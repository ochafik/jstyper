  "scripts": {
    "demo": "concurrently --kill-others \"npm run rollup:w\" lite-server",
    "start": "concurrently --kill-others \"npm run demo\" \"npm run test:w\"",
    "lite": "lite-server",
    "rollup": "rollup -c",
    "rollup:w": "rollup -c -w",
    "build": "npm run rollup && inline-assets index.html build/index.html",
    "update-tests": "UPDATE_GOLDENS=1 npm test",
    "test:w": "tsc && concurrently \"tsc -w\" \"mocha build/test --watch\"",
