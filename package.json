{
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^2.2.39",
    "@types/node": "^7.0.4",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "chai": "^3.5.0",
    "concurrently": "^3.1.0",
    "es6-promisify": "^5.0.0",
    "inline-assets": "^1.1.2",
    "lite-server": "^2.2.2",
    "mocha": "^3.2.0",
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-filesize": "^1.0.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^3.2.2",
    "typescript": "^2.1.5",
    "uglify-js-harmony": "^2.6.2",
    "watch": "^1.0.1"
  },
  "name": "jstyper",
  "version": "0.1.0",
  "main": "build/src/typer.js",
  "dependencies": {
    "minimist": "^1.2.0"
  },
  "scripts": {
    "build": "npm run rollup && inline-assets index.html build/index.html",
    "demo": "concurrently --kill-others \"npm run rollup:w\" lite-server",
    "start": "concurrently --kill-others \"npm run demo\" \"npm run test:w\"",
    "rollup": "rollup -c",
    "rollup:w": "rollup -c -w",
    "test": "tsc && mocha build/test",
    "test:w": "tsc && concurrently \"tsc -w\" \"mocha build/test --watch\"",
    "update-specs": "UPDATE_SPECS=1 npm test",
    "update-specs:w": "UPDATE_SPECS=1 npm test:w",
    "patch-release": "npm version patch && npm publish && git push --follow-tags"
  },
  "author": "",
  "license": "ISC",
  "bin": {
    "jstyper": "bin/jstyper.js"
  }
}
